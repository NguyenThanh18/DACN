

@{
    ViewBag.Title = "QLTK";
    var listTK = ViewBag.QLTK;
}

<link href="~/Content/Admin/css/all.min.css" rel="stylesheet" type="text/css">
<link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
      rel="stylesheet">

<!-- Custom styles for this template-->
<link href="~/Content/Admin/css/sb-admin-2.min.css" rel="stylesheet">
<div class="container-fluid">
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Position</th>
                            <th>Office</th>
                            <th>Age</th>
                            <th>Start date</th>
                            <th>Salary</th>
                        </tr>
                    </thead>
                    <tfoot>
                        <tr>
                            <th>Name</th>
                            <th>Email</th>
                            <th>SDT</th>
                            <th>Ngay Tao</th>
                            <th>Ho Ten</th>
                            <th>Salary</th>
                        </tr>
                    </tfoot>
                    <tbody>
                        @foreach (var item in listTK)
                        {
                            <tr>
                                <td>@item.Username</td>
                                <td>@item.Email</td>
                                <td>@item.SDT</td>
                                <td>@item.NgayTao</td>
                                <td>@item.HoTen</td>
                                <td>1</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<script src="~/Content/Admin/js/jquery.min.js"></script>
<script src="~/Content/Admin/js/bootstrap.bundle.min.js"></script>

<!-- Core plugin JavaScript-->
<script src="~/Content/Admin/js/jquery.easing.min.js"></script>

<!-- Custom scripts for all pages-->
<script src="~/Content/Admin/js/sb-admin-2.min.js"></script>



